{
  "name": "mclab",
  "version": "0.0.1",
  "description": "McLab Web IDE.",
  "main": "js/app.js",
  "dependencies": {
    "flux": "^2.1.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.13.3",
    "react-dropzone": "^2.1.0"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jest-cli": "^0.4.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify -o static/bundle.js -v -d js/app.js",
    "build": "browserify -x react . -t [envify --NODE_ENV production] [babelify --stage 0]  | uglifyjs -cm > static/bundle.min.js"
  },
  "author": "Deepanjan Roy",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0
        }
      ],
      "reactify",
      "envify"
    ]
  }
}
