{
  "name": "mclab",
  "version": "0.0.1",
  "description": "McLab Web IDE.",
  "main": "./js/app.js",
  "repository": "https://github.com/deepanjanroy/mclab-web",
  "dependencies": {
    "async": "^1.5.2",
    "express": "~4.13.4",
    "flux": "^2.1.1",
    "immutable": "^3.7.5",
    "key-mirror-nested": "^1.2.4",
    "multer": "^1.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.14.0",
    "react-bootstrap": "^0.27.2",
    "react-dom": "^0.14.0",
    "react-dropzone": "^2.1.0",
    "react-select": "^0.8.2",
    "superagent": "^1.4.0",
    "unzip": "^0.1.11",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^6.2.0",
    "classnames": "^2.2.0",
    "envify": "^3.0.0",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "npm run watch && npm run startserver",
    "startserver": "node mcnode/app.js",
    "watch": "watchify -o ./static/bundle.js -v -d ./js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] [babelify --stage 0]  | uglifyjs -cm > ./static/bundle.min.js"
  },
  "author": "Deepanjan Roy",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0
        }
      ],
      "reactify",
      "envify"
    ]
  }
}
